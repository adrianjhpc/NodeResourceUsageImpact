SRCINJ  = core_management.c inject_usage.c inject_memory.c inject_cpu.c inject_io.c inject_network.c

OBJINJ  =$(SRCINJ:.c=.o)

CC	= icc
PP	= -g 

CFLAGS	= -O3 $(PP) 
LIBS	= -lpthread 


PRGINJ  = inject

main:	$(PRGINJ)

.c.o:	Makefile
	$(CC) -c $(CFLAGS) $(ELIBS) $<

$(PRGINJ):$(OBJINJ) Makefile.inject inject_definitions.h
	$(CC) $(LDFLAGS) -o $@ $(OBJINJ) $(LIBS)

clean:
	rm -fr $(TMP) $(OBJINJ) $(PRGINJ) core
